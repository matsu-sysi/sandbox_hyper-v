{
  "packages": [
    {
      "id": "Git.Git",
      "version": "2.53.0",
      "command": "git",
      "autoInstall": true,
      "installerFile": "Git-2.53.0-64-bit.exe",
      "installerType": "exe",
      "silentArgs": "/VERYSILENT /NORESTART /NOCANCEL",
      "timeoutSec": 300
    },
    {
      "id": "Microsoft.VisualStudioCode",
      "version": "latest-stable",
      "command": "code",
      "autoInstall": true,
      "installerFile": "VSCodeUserSetup-x64-latest.exe",
      "installerType": "exe",
      "silentArgs": "/VERYSILENT /NORESTART /MERGETASKS=!runcode",
      "timeoutSec": 300
    },
    {
      "id": "Python.Python.3.13",
      "version": "3.13.12",
      "command": "python",
      "autoInstall": false,
      "installerFile": "python-3.13.12-amd64.exe",
      "installerType": "exe",
      "silentArgs": "/quiet InstallAllUsers=0 PrependPath=1 Include_test=0",
      "timeoutSec": 300
    },
    {
      "id": "OpenJS.NodeJS.LTS",
      "version": "24.13.1",
      "command": "node",
      "autoInstall": false,
      "installerFile": "node-v24.13.1-x64.msi",
      "installerType": "msi",
      "silentArgs": "/qn /norestart",
      "timeoutSec": 300
    }
  ],
  "vscodeExtensions": [
    "ms-python.python",
    "esbenp.prettier-vscode",
    "dbaeumer.vscode-eslint"
  ],
  "useWingetFallback": false
}
